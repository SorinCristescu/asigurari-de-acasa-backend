{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "posts",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "string"
    },
    "text": {
      "type": "richtext"
    },
    "image_url": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "posts"
    },
    "keywords": {
      "type": "text"
    }
  }
}
